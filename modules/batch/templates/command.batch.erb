#!/usr/bin/env zsh
# This file is managed by puppet.

PATH="/usr/local/bin:/usr/local/sbin:/usr/bin:/usr/sbin:/bin:/sbin"

echo >> /var/log/batch-jobs.log
date >> /var/log/batch-jobs.log
date '+%s' >> /var/log/batch-jobs.log
echo <%= @command %> >> /var/log/batch-jobs.log
<%= @command %> >> /var/log/batch-jobs.log 2>&1
